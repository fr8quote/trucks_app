<div class="media">
  <div class="media-body">
    <table class="table table-hover">
      <thead>
        <tr>
      <th>Status</th>
      <th>Service Time</th>
      <th>Notes</th>
      <th>Time group</th>
      <th>Status record</th>
      <th>User ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @status_times.each do |status_time| %>
      <tr>
        <td><%= status_time.status %></td>
        <td><%= status_time.created_at.strftime("%b %d, %Y || %I:%M:%S:%p") %></td>
        <td><%= status_time.notes %></td>
        <td><%= status_time.time_group_id %></td>
        
        <td><%= status_time.user_id %></td>
        <td><%= link_to 'Show', status_time, class: 'btn btn-mini' %></td>
        <td><%= link_to 'Edit', edit_status_time_path(status_time), class: 'btn btn-mini btn-success' %></td>
        <td><%= link_to 'Destroy', status_time, method: :delete, class: 'btn btn-mini btn-danger', data: { confirm: 'Are you sure?' } %></td>
    </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
<br>

